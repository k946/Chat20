<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!--设置弧形-->
    <corners android:radius="30dip" />

    <!--设置颜色-->
    <solid android:color="@color/blue"  />
    <stroke android:width="1dip" android:color="#fefefe" />
    <!--设置间隔-->
    <padding android:bottom="10dp"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp"/>
</shape>
